# font_family      Fira Code
# font_family      Hack Nerd Font Mono
# font_family      JetBrainsMono NF 
# font_family        Iosevka Nerdfont         
# font_family      MesloLGL Nerd Font
# font_family      family="Source Code Pro"
font_family      family='JetBrainsMono Nerd Font' postscript_name=JetBrainsMonoNF-Regular
bold_font        auto
italic_font      auto
bold_italic_font auto
bold_font        no
italic_font      auto
bold_italic_font auto
font_size 13
text_composition_strategy legacy
disable_ligatures always

enable_audio_bell no
cursor_shape block
shell_integration no-cursor
map f1 new_window_with_cwd

## dark pastel, dark one nuanced

# BEGIN_KITTY_THEME
# Monokai Soda
include current-theme.conf
# END_KITTY_THEME


